{
  "name": "@tailify/eslint-config-react",
  "version": "1.4.0",
  "description": "Tailify's very own opinionated ESLint configuration for React",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "npm run lint && npm run test-only",
    "test-only": "jest"
  },
  "repository": "tailify/eslint-config-react",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "license": "UNLICENSED",
  "bugs": "https://github.com/tailify/eslint-config-react/issues",
  "homepage": "https://github.com/tailify/eslint-config-react#readme",
  "dependencies": {
    "@tailify/eslint-config": "3.0.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.2"
  },
  "devDependencies": {
    "@tailify/eslint-config-legacy": "^1.0.0",
    "@tailify/prettier-config": "^1.0.0",
    "eslint": "^5.9.0",
    "jest": "^23.6.0",
    "prettier": "^1.15.0",
    "react": "^16.7.0"
  },
  "peerDependencies": {
    "eslint": "^5.9.0",
    "prettier": "^1.15.0"
  },
  "engines": {
    "node": ">=8.3"
  },
  "greenkeeper": {
    "commitMessages": {
      "dependencyUpdate": "Update ${dependency} to version ${version}",
      "devDependencyUpdate": "Update ${dependency} to version ${version}",
      "dependencyPin": "Pin ${dependency} to ${oldVersion}",
      "devDependencyPin": "Pin ${dependency} to ${oldVersion}"
    },
    "prTitles": {
      "basicPR": "Update ${dependency} to the latest version",
      "groupPR": "Update ${dependency} in group ${group} to the latest version"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
